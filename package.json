{
  "name": "svgo",
  "version": "1.0.0-alpha.1",
  "description": "Nodejs-based tool for optimizing SVG vector graphics files",
  "keywords": [ "svg", "optimize", "minify" ],
  "homepage": "https://github.com/svg/svgo",
  "bugs": {
    "url": "https://github.com/svg/svgo/issues"
  },
  "author": "Kir Belevich <kir@soulshine.in> (https://github.com/deepsweet)",
  "contributors": [
      "Sergey Belov <peimei@ya.ru> (http://github.com/arikon)",
      "Lev Solntsev <lev.sun@ya.ru> (http://github.com/GreLI)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/svg/svgo.git"
  },
  "bin": {
    "svgo": "./bin/svgo"
  },
  "main": "./index.js",
  "dependencies": {
    "commander": "2.5.x",
    "sax": "0.6.x"
  },
  "devDependencies": {
    "eslint": "0.10.x",
    "jscs": "1.8.x",
    "mocha": "2.0.x",
    "chai": "1.10.x",
    "unit-coverage": "3.2.x",
    "coveralls": "2.11.x"
  },
  "scripts": {
    "eslint": "eslint *.js lib/ test/",
    "jscs": "jscs *.js lib/ test/",
    "lint": "npm run eslint && npm run jscs",
    "unit": "mocha",
    "test": "npm run lint && npm run unit",
    "coverage": "unit-coverage run -s 'lib/**' -t 'test/**' -r lcov -o coverage/lcov.info",
    "coverage-html": "unit-coverage run -s 'lib/**' -t 'test/**' -r html -o coverage/index.html",
    "coveralls": "unit-coverage run -q -s 'lib/**' -t 'test/**' -r lcov | coveralls",
    "travis": "npm run test && npm run coveralls"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "MIT"
}
